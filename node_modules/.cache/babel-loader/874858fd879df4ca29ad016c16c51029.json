             </el-select>\n            </el-form-item>\n            <el-form-item>\n                <el-button type=\"primary\" @click=\"onSubmit\">查询</el-button>\n            </el-form-item>\n        </el-form>\n        </el-card>\n        <el-dialog class=\"dialog-m\" title=\"新增分类\" :visible.sync=\"vis\" @close='closeDialog'>\n            <el-form :model=\"newForm\">\n               \n                <el-form-item label=\"上级分类\" :label-width=\"formLabelWidth\">\n                <el-select v-model=\"newForm.region\" placeholder=\"请选分类\">\n                    <el-option label=\"水果\" value=\"shanghai\"></el-option>\n                </el-select>\n                </el-form-item>\n                 <el-form-item label=\"分类名称\" :label-width=\"formLabelWidth\">\n                    <el-input v-model=\"newForm.name\" auto-complete=\"off\"></el-input>\n                </el-form-item>\n                 <el-form-item label=\"分类图标\" :label-width=\"formLabelWidth\">\n                     <el-upload\n                        action=\"https://jsonplaceholder.typicode.com/posts/\"\n                        list-type=\"picture-card\"\n                        :on-preview=\"handlePictureCardPreview\"\n                        :on-remove=\"handleRemove\">\n                        <i class=\"el-icon-plus\"></i>\n                    </el-upload>\n                    <el-dialog :visible.sync=\"dialogVisible\" size=\"tiny\">\n                        <img width=\"100%\" :src=\"dialogImageUrl\" alt=\"\">\n                    </el-dialog>\n                </el-form-item>\n                 <el-form-item label=\"排序\" :label-width=\"formLabelWidth\">\n                    <el-input v-model=\"newForm.px\" auto-complete=\"off\"></el-input>\n                </el-form-item>\n                 <el-form-item label=\"商品参数\" :label-width=\"formLabelWidth\">\n                     <el-popover\n                        class=\"popv\"\n                        ref=\"popover4\"\n                        placement=\"right\"\n                        width=\"250\"\n                        v-model=\"visible2\"\n                        trigger=\"click\">\n                        <el-form :model=\"csForm\" class=\"cs-form\">\n                            <p>添加参数</p>\n                            <el-form-item >\n                                <el-input v-model=\"csForm.cs\" placeholder=\"请输入参数名称\"></el-input>\n                            </el-form-item>\n                            <el-form-item class=\"pop-box\">\n                                <el-button type=\"text\" @click=\"addCS(0)\">取消</el-button>\n                                <el-button size = mini type=\"primary\" @click=\"addCS(1)\">确定</el-button>\n                            </el-form-item>\n                        </el-form>\n                    </el-popover>\n                    <el-button v-popover:popover4 size =  mini type=\"primary\" icon=\"el-icon-circle-plus-outline\">商品参数</el-button>\n                    <span class=\"tip-text\">（设置的商品参数，在添加商品，选择此分类时展示）</span>\n                </el-form-item>\n                <el-form-item :label-width=\"formLabelWidth\">\n                    <el-tag\n                        v-for=\"tag in tags\"\n                        :key=\"tag.name\"\n                        closable\n                        @close=\"handleClose(tag)\"\n                        :type=\"tag.type\">\n                        {{tag.name}}\n                    </el-tag>\n                </el-form-item>\n            </el-form>\n            <div slot=\"footer\" class=\"dialog-footer\">\n                <el-button @click=\"onCancel\">取 消</el-button>\n                <el-button type=\"primary\" @click=\"onOk\">确 定</el-button>\n            </div>\n        </el-dialog>\n    </div>\n</template>\n\n<script>\n\nexport default {\n    props:{\n        form:{\n            type:Object,\n            required:true,\n            default:()=>{\n                return {\n\n                }\n            }\n        },\n        newForm:{\n            type: Object,\n            default:() => {\n                return{\n\n                }\n            }\n        },\n        dialogFormVisible:{\n            type:Boolean,\n            default:()=>{\n                return false\n            }\n        }\n    },\n    data(){\n        return{\n            inline:true,\n            classifications:[\n                '全部','水果'\n           ],\n            formLabelWidth: '120px',\n             dialogImageUrl: '',\n            dialogVisible: false,\n            csForm:{},\n            visible2: false,\n            tags: [\n            ],\n            vis:false\n        }\n        \n    },\n     watch: {\n        dialogFormVisible (newValue, oldValue) {\n        this.vis = newValue\n        }\n    },\n    methods:{\n      onSubmit(){\n          this.$emit('search')\n      },\n      addNewClass(){\n          this.$emit('addNewClass')\n       \n      },\n      closeDialog(){\n           this.$emit('addNewClass') \n      },\n      onOk(){\n           this.vis = false\n      },\n      onCancel(){\n         this.vis = false \n      },\n      handleRemove(file, fileList) {\n        console.log(file, fileList);\n      },\n      handlePictureCardPreview(file) {\n        this.dialogImageUrl = file.url;\n        this.dialogVisible = true;\n      },\n      addCS(type){\n          \n            if (type === 1 && this.csForm.cs) {\n\n                this.tags.push({name: this.csForm.cs})\n                this.visible2 = !this.visible2;\n                console.log(this.tags);\n                \n            }\n            if(type === 1 && !this.csForm.cs){\n                    this.$message('请添加参数名称');\n            }\n            if (type === 0) {\n                this.visible2 = !this.visible2;\n            }\n            \n            this.csForm = {}\n            \n      },\n      handleClose(tag) {\n        this.tags.splice(this.tags.indexOf(tag), 1);\n      },\n      \n    },\n    created(){\n        \n    }\n}\n</script>\n<style>\n.form-ctn {\n    flex: 1;\n    background-color: #fff;\n}\n.form-ctn .box-card {\n    margin-top: 15px;\n}\n.clearfix {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n}\n.tip-text {\n    font-size: 14px;\n    color: #fe4c56;\n}\n.dialog-m .el-input {\n    width: 60%;\n}\n.cs-form .el-input {\n     width: 90%;\n}\n.pop-box{\n    display: flex;\n    justify-content: flex-end;\n    margin-top: 20px;\n}\n.dialog-m .el-tag {\n    margin-right: 10px;\n}\n.left-ctn .el-card {\n    overflow-y: scroll;\n}\n</style>\n\n"],"sourceRoot":"src/components"}]}